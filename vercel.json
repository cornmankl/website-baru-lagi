{\n  "version": 2,\n  "name": "cornman-ecommerce",\n  "routes": [\n    {\n      "src": "/api/webhooks/(.*)",\n      "dest": "/api/webhooks/$1"\n    },\n    {\n      "src": "/api/(.*)",\n      "dest": "/api/$1"\n    },\n    {\n      "src": "/admin/(.*)",\n      "dest": "/admin/$1"\n    },\n    {\n      "src": "/products/(.*)",\n      "dest": "/products/$1"\n    },\n    {\n      "src": "/checkout",\n      "dest": "/checkout"\n    },\n    {\n      "src": "/login",\n      "dest": "/login"\n    },\n    {\n      "src": "/(.*)",\n      "dest": "/$1"\n    }\n  ],\n  "env": {\n    "NODE_ENV": "production"\n  },\n  "build": {\n    "env": {\n      "NODE_ENV": "production"\n    }\n  },\n  "functions": {\n    "api/webhooks/**/*": {\n      "maxDuration": 30\n    },\n    "api/**/*": {\n      "maxDuration": 10\n    }\n  },\n  "crons": []\n}